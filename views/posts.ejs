<!doctype html>
<html>
<head>
	<% include partials/head %>
</head>
<body>
<header>
    <% include partials/nav %>
</header>
<div class="container">
  <div class="row">
    <div class="col-md-12">
        <% posts.forEach(function(post){%>
          <div class="well">
            <h4><%= post.posted.toLocaleDateString("en-US") %></h4>
            <h1><%= post.title %></h1>
            <h3>By: <%= post.user.email %></h3>
            <p><%= post.content %></p>
            <a href="#"><button class="btn btn-primary">View Profile</button></a>
          </div>
        <%});%>
    </div>
  </div>
</div>
</body>
</html>
